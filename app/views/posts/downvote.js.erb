<% if current_user.disliked? @post %>
 $('.downvote-btn-<%= @post.id %>').removeClass('alert-info').addClass('alert-warning')
 $('.upvote-btn-<%= @post.id %>').removeClass('alert-warning')
	<% else %>
 $('.downvote-btn-<%= @post.id %>').removeClass('alert-warning').addClass('alert-info')
	<% end %>
$('.likes-count-<%= @post.id %>').html("<%=@post.weighted_score %>")